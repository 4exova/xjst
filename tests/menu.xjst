c.name == 'page'
    return '<html>' +
        '<head></head>' +
        '<body>' +
            c.apply({ apply: c.apply, page: c, name: 'menu', val: c.val.menu }) +
        '</body>' +
    '</html>'

c.name == 'menu'
    return '<ul class="menu">' +
        c.val.map(function(v){
            return c.apply({
                apply: c.apply,
                page: c.page,
                parent: c.name,
                name: 'item',
                val: v }) }).join('') +
    '</ul>'

c.parent == 'menu' && c.name == 'item'
    return '<li>' +
        c.apply({
            'bla-bla': c['bla-bla'],
            apply: c.apply,
            page: c.page,
            name: 'menu item content',
            val: c.val }) +
    '</li>'

c.name == 'menu item content'
    return '<a href="' + c.val.link + '">' +
        c.val.text +
    '</a>'

c.name == 'menu item content' && c.val.id == c.page.val.id
    return '<span>' +
        c.val.text +
    '</span>'

c.bla1 == 'bla1' && c.bla2 == 'bla2'
    return 'bla'
